<%= render partial: "pages/ijuryheader" %>

<h1>Create a New Case</h1>
<%= form_tag cases_path do %>
  <%= label_tag "Title" %>
  <%= text_field_tag :title %>
  <%= label_tag "Summary" %>
  <%= text_field_tag :summary %>
  <%= label_tag "Defendant" %>
  <%= text_field_tag :defendant_user_name %>

  <%= submit_tag "Create Case" %>
<% end %>



<%= render partial: "pages/ijuryfooter" %>